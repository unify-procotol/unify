{
  "name": "layout-registry",
  "version": "0.0.1",
  "description": "shadcn/ui compatible component registry server for URPC layout components",
  "main": "server.js",
  "scripts": {
    "registry:build": "shadcn build --output public/r",
    "registry:serve": "node server.js",
    "registry:dev": "npm run registry:build && npm run registry:serve",
    "start": "npm run registry:serve",
    "dev": "npm run registry:dev"
  },
  "keywords": [
    "urpc",
    "layouts",
    "components",
    "registry",
    "unilab",
    "react",
    "tailwindcss",
    "shadcn",
    "shadcn-ui"
  ],
  "author": "Unilab",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unify-procotol/unify.git",
    "directory": "packages/layout-installer"
  },
  "homepage": "https://github.com/unify-procotol/unify/tree/main/packages/layout-installer#readme",
  "bugs": {
    "url": "https://github.com/unify-procotol/unify/issues"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "shadcn": "^2.9.0-canary.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0"
  },
  "files": [
    "server.js",
    "registry.json",
